public class demo {
	private Object ob;
	public demo(Object ob){
		this.ob=ob;
	}
	public Object getOb(){
		return ob;
	}
	public void setOb(Object ob){
		this.ob=ob;
		
	}
	public void showType(){
		System.out.println("the true type is:"+ob.getClass().getName());
		
	}
}
